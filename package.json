{
  "name": "dnc-vinny",
  "version": "1.0.0",
  "description": "DNC VINNY - Do Not Call Management System for VinSolutions and PossibleNOW Integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "config": "ts-node src/config/cli/config-cli.ts",
    "config:init": "ts-node src/config/cli/config-cli.ts init",
    "config:validate": "ts-node src/config/cli/config-cli.ts validate",
    "config:test": "ts-node src/config/cli/config-cli.ts test",
    "config:encrypt": "ts-node src/config/cli/config-cli.ts encrypt",
    "config:decrypt": "ts-node src/config/cli/config-cli.ts decrypt",
    "example": "ts-node src/config/examples/usage-example.ts"
  },
  "keywords": [
    "dnc",
    "vinsolutions",
    "possiblenow",
    "automotive",
    "crm",
    "integration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "js-yaml": "^4.1.0",
    "semver": "^7.5.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}